# Variáveis de ambiente para FIPE API
PORT=3000
NODE_ENV=development

# JWT Secret
JWT_SECRET=sua_chave_secreta_muito_forte_aqui_para_fipe_api_123456789

# Configurações FIPE
FIPE_CACHE_TIME=3600
FIPE_REQUEST_TIMEOUT=10000
FIPE_MAX_RETRIES=3
FIPE_MAX_SEARCH_RESULTS=100

# Configurações da aplicação
MAX_FAVORITES=100
MAX_HISTORY_ITEMS=1000
MAX_COMPARISONS=5

# Rate limiting
RATE_LIMIT_WINDOW=900000
RATE_LIMIT_MAX=1000
